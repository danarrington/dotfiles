{
  /**
   * Better Defaults
   **/
  "window.nativeFullScreen": false, // non-workspace full screen

  /**
   * Hide Everything
   **/
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.lineNumbers": "on",
  "workbench.sideBar.location": "left",

  /**
   * Silence The Noise
   */
  // "scm.diffDecorations": "none",
  // "editor.hover.enabled": false,
  "editor.matchBrackets": "never",
  // "workbench.tips.enabled": false,
  // "editor.colorDecorators": false,
  "git.decorations.enabled": false,
  // "workbench.startupEditor": "none",
  // "editor.lightbulb.enabled": false,
  "editor.selectionHighlight": false,
  // "editor.overviewRulerBorder": false,
  "editor.occurrencesHighlight": false,
  // "problems.decorations.enabled": false,
  "editor.renderControlCharacters": false,
  // "editor.hideCursorInOverviewRuler": true,
  // "editor.gotoLocation.multipleReferences":"goto",
  // "editor.gotoLocation.multipleDefinitions": "goto",
  // "editor.gotoLocation.multipleDeclarations": "goto",
  // "editor.gotoLocation.multipleImplementations": "goto",
  // "editor.gotoLocation.multipleTypeDefinitions": "goto",

  /**
   * Typography
   **/
  // "workbench.colorTheme": "Palenight Theme",
  // "editor.fontSize": 15,
  // "editor.lineHeight": 45,
  // "editor.suggestFontSize": 15,
  // "editor.suggestLineHeight": 28,
  // "terminal.integrated.fontSize": 15,
  // "terminal.integrated.lineHeight": 1.5,

  /**
   * Display Settings
   **/
  "breadcrumbs.enabled": false,
  "editor.fontFamily": "JetBrains Mono",
  "editor.renderLineHighlight": "none",
  "editor.rulers": [100],
  "workbench.colorTheme": "Dracula Pro (Van Helsing)",
  "workbench.colorCustomizations": {
    "editorLineNumber.foreground": "#444",
    "editorBracketHighlight.foreground1": "#fff",
    "editorBracketHighlight.foreground2": "#FFDF00",
    "editorBracketHighlight.foreground3": "#87CEFA",
    "editorBracketHighlight.foreground4": "#9370DB"
  },

  /**
   * Almost get rid of tabs
   **/
  // "workbench.editor.showTabs": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.limit.value": 1,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,

  /**
   * Legacy Settings
   **/
  "[go]": {},
  "clang-format.style": "{BasedOnStyle: google, IndentWidth: 4}",
  "editor.folding": false,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.insertSpaces": true,
  "editor.parameterHints.enabled": false,
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": false,
  "files.insertFinalNewline": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "gitlens.historyExplorer.enabled": true,
  "gitlens.keymap": "alternate",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "go.formatTool": "goimports",
  "go.inferGopath": true,
  "go.lintFlags": ["-config", "/Users/dan/go/revive.toml"],
  "go.lintTool": "revive",
  "go.testFlags": [],
  "go.toolsGopath": "/Users/dan/go",
  "go.useLanguageServer": true,
  "vim.disableAnnoyingNeovimMessage": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": [],
      "before": ["<C-n>"],
      "commands": [
        {
          "args": [],
          "command": ":nohl"
        }
      ]
    },
    {
      "after": [],
      "before": ["leader", "s"],
      "commands": [
        {
          "args": [],
          "command": "workbench.action.files.save"
        }
      ]
    },
    {
      "after": [],
      "before": ["leader", "q"],
      "commands": [
        {
          "args": [],
          "command": "workbench.action.closeActiveEditor"
        }
      ]
    },
    // {
    //     "after": [],
    //     "before": [
    //         "<C-h>"
    //     ],
    //     "commands": [
    //         {
    //             "args": [],
    //             "command": "workbench.action.focusLeftGroup"
    //         }
    //     ]
    // },
    // {
    //     "after": [],
    //     "before": [
    //         "<C-l>"
    //     ],
    //     "commands": [
    //         {
    //             "args": [],
    //             "command": "workbench.action.focusRightGroup"
    //         }
    //     ]
    // },
    {
      "after": [],
      "before": ["leader", "r"],
      "commands": [
        {
          "args": [],
          "command": "go.test.cursor"
        }
      ]
    },
    {
      "after": [],
      "before": ["leader", "t"],
      "commands": [
        {
          "args": [],
          "command": "go.test.file"
        }
      ]
    },
    {
      "after": [],
      "before": ["leader", "l"],
      "commands": [
        {
          "args": [],
          "command": "go.test.previous"
        }
      ]
    },
    {
      "after": [],
      "before": ["leader", "<enter>"],
      "commands": [
        {
          "args": [],
          "command": "editor.action.goToDeclaration"
        }
      ]
    },
    {
      "after": ["<", "g", "v"],
      "before": ["S-Tab"]
    },
    {
      "after": [">", "g", "v"],
      "before": ["Tab"]
    },
    {
      "after": [],
      "before": ["leader", "`"],
      "commands": [
        {
          "args": [],
          "command": "workbench.action.terminal.toggleTerminal"
        }
      ]
    },
    {
      "after": [],
      "before": ["leader", "d"],
      "commands": [
        {
          "args": [],
          "command": "workbench.action.splitEditorDown"
        }
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "after": ["\"", "_", "d", "P"],
      "before": ["leader", "r"]
    }
  ],
  "window.zoomLevel": 2,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.panel.location": "bottom",
  "eslint.format.enable": true,
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/.next": true,
    "**/dist": true
  },
  "eslint.alwaysShowStatus": true,
  "gitlens.codeLens.enabled": false,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "git.confirmSync": false,
  "git.autofetch": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active"
  /**
   * / Legacy Settings
   * Anything below there has been added somewhat recently
   **/
}
